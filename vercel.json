{
  "version": 2,
  "name": "my-project",
  "builds": [
    {
      "src": "api.js",
      "use": "@vercel/node"
    },
    {
      "src": "main.js",
      "use": "@vercel/node"
    }
  ],
  "scripts": {
   "start": "npm start"
    }
  "routes": [
    {
      "src": "/api/*",
      "dest": "api.js"
    },
    {
      "src": "/*",
      "dest": "main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
